{
    "name": "graham-campbell/gitlab",
    "description": "GitLab Is A GitLab Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "gitlab", "php-gitlab-api", "PHP GitLab API", "gitlab bridge", "bridge", "GitLab", "Laravel GitLab", "Laravel-GitLab", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "graham-campbell/cache-plugin": "^1.0",
        "graham-campbell/manager": "^4.2",
        "m4tthumphrey/php-gitlab-api": "dev-master"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "madewithlove/illuminate-psr-cache-bridge": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "suggest": {
        "madewithlove/illuminate-psr-cache-bridge": "Allows caching GitLab HTTP requests"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitLab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\GitLab\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9-dev"
        },
        "laravel": {
            "providers": [
                "GrahamCampbell\\GitLab\\GitLabServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
